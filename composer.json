{
    "name": "builtbybuilder/easynavplus",
    "description": "Making managing navigation in Laravel easy.",
    "keywords": ["nav", "easy", "laravel", "easynavplus", "helpers", "active", "builtbybuilder"],
    "license": "MIT",
    "homepage": "https://github.com/builtbybuilder/LaravelEasyNavPlus",
    "support": {
        "issues": "https://github.com/builtbybuilder/LaravelEasyNavPlus/issues",
        "source": "https://github.com/builtbybuilder/LaravelEasyNavPlus"
    },
    "authors": [
        {
            "name": "BuiltByBuilder",
            "email": "support@builtbybuilder.com"
        }
    ],
    "require": {
      "php": ">=5.5.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.7.*"
    },
    "autoload": {
        "psr-0": {
            "BuiltByBuilder\\EasyNavPlus": "src/"
        },
        "files": [
            "src/BuiltByBuilder/EasyNavPlus/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "BuiltByBuilder\\EasyNavPlus\\EasyNavPlusServiceProvider"
            ],
            "aliases": {
                "Nav": "BuiltByBuilder\\EasyNavPlus\\EasyNavPlusFacade"
            }
        }
    }
}
